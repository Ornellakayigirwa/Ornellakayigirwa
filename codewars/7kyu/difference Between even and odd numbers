function solve(a){
 let evenCount = 0
  let oddCount = 0
  let difference;
  
  a.filter(count =>{
    if(typeof count === 'number'){
    if(count % 2 === 0){
      evenCount ++
    } else if (!count % 2 === 0){
      oddCount ++;
    }
  }
    })
  
  difference =  evenCount - oddCount
  
  return difference
};